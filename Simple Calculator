#include<stdio.h>
int main()
{
  char ch;
  double num1,num2;
  scanf("%c %lf %lf",&ch,&num1,&num2);
  switch(ch)
   {
     case '+' :
     printf("%lf \n",num1+num2);
     break;

     case '-' :
     printf("%lf\n",num1-num2);
     break;

     case '*' :
     printf("%lf\n",num1*num2);
     break;

     case '/' :
     if(num2!=0){
        printf("%lf \n",num1/num2);
     } else{
        printf("Error\n");
     }
     break;

     default :
     printf("Invalid operator\n.");

    }
     return 0;


    }









